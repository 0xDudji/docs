{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "linden",
  "name": "Dudji's Docs",
  "colors": {
    "primary": "#d5363e",
    "light": "#e05560",
    "dark": "#b02a32"
  },
  "appearance": {
    "default": "dark",
    "decorations": {
      "background": "grid"
    }
  },
  "background": {
    "decoration": "windows",
    "color": {
      "dark": "#060606"
    }
  },
  "styling": {
    "codeblocks": {
      "theme": {
        "dark": "tokyo-night",
        "light": "github-light"
      }
    }
  },
  "favicon": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "custom": {
    "css": "/custom.css"
  },
  "seo": {
    "indexHiddenPages": false
  },
  "fonts": {
    "body": { "family": "JetBrains Mono" },
    "heading": { "family": "JetBrains Mono" }
  },
  "routing": {
    "prefix": "/docs"
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Home",
        "groups": [
          {
            "group": "Home",
            "pages": ["index"]
          }
        ]
      },
      {
        "tab": "Playbook",
        "groups": [
          {
            "group": "Overview",
            "pages": ["playbook/index"]
          },
          {
            "group": "Recon",
            "pages": ["playbook/recon/index"]
          },
          {
            "group": "Enumeration",
            "pages": [
              "playbook/enumeration/index",
              "playbook/enumeration/web-enumeration"
            ]
          },
          {
            "group": "Exploitation",
            "pages": ["playbook/exploitation/index"]
          },
          {
            "group": "Post-Exploitation",
            "pages": ["playbook/post-exploitation/index"]
          }
        ]
      },
      {
        "tab": "Attacks",
        "groups": [
          {
            "group": "Overview",
            "pages": ["attacks/index"]
          },
          {
            "group": "Web",
            "pages": [
              "attacks/web/index",
              "attacks/web/local-file-inclusion",
              "attacks/web/sql-injection"
            ]
          },
          {
            "group": "Privilege Escalation",
            "pages": [
              "attacks/privilege-escalation/index",
              "attacks/privilege-escalation/linux",
              "attacks/privilege-escalation/windows"
            ]
          }
        ]
      },
      {
        "tab": "Tech",
        "groups": [
          {
            "group": "Overview",
            "pages": ["tech/index"]
          },
          {
            "group": "Linux",
            "pages": [
              "tech/linux/index",
              "tech/linux/system-fundamentals",
              "tech/linux/authentication-authorization",
              "tech/linux/system-services",
              "tech/linux/storage-devices"
            ]
          }
        ]
      },
      {
        "tab": "Payloads",
        "groups": [
          {
            "group": "Overview",
            "pages": ["payload/index"]
          }
        ]
      },
      {
        "tab": "Tools",
        "groups": [
          {
            "group": "Overview",
            "pages": ["tools/index"]
          }
        ]
      },
      {
        "tab": "Writeups",
        "groups": [
          {
            "group": "Overview",
            "pages": ["writeups/index"]
          },
          {
            "group": "Categories",
            "pages": [
              "writeups/machines/index",
              "writeups/Challenges/index",
              "writeups/CTFs/index"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": []
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg",
    "href": "https://dudji.com"
  },
  "footer": {
    "links": [
      {
        "header": "Dudji",
        "items": [
          { "label": "Home", "href": "https://dudji.com" },
          { "label": "Contact", "href": "https://dudji.com/contact" }
        ]
