      {
        "header": "Hackbook",
        "items": [
          { "label": "Playbook", "href": "/docs/playbook" },
          { "label": "Attacks", "href": "/docs/attacks" },
          { "label": "Tech", "href": "/docs/tech" },
          { "label": "Payloads", "href": "/docs/payload" },
          { "label": "Tools", "href": "/docs/tools" }
        ]
      },
      {
        "header": "Writeups",
        "items": [
          { "label": "Machines", "href": "/docs/writeups/machines" },
          { "label": "Challenges", "href": "/docs/writeups/challenges" },
          { "label": "CTFs", "href": "/docs/writeups/ctfs" }
        ]
      }
    ],
    "socials": {
      "x": "https://x.com/dudji",
      "linkedin": "https://linkedin.com/in/dudji",
      "instagram": "https://instagram.com/dudji",
      "github": "https://github.com/0xDudji"
    }
  }
}
